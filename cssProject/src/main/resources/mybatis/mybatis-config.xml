		<typeAlias type="com.jdl.css.approval.model.vo.ApprovalConditionVo" alias="AppConVo" />
	</typeAliases>

	<mappers>
		<!-- <mapper resource="mybatis/sql/testMapper.xml"/> -->
		<mapper resource="mybatis/sql/employeeMapper.xml" />
